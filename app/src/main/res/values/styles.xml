<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="@android:style/Theme.NoTitleBar">
    </style>

    <style name="Widget.SeekBar.Normal" parent="@android:style/Widget.SeekBar">
        <item name="android:maxHeight">6.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/seekbar_process_drawable_green</item>
        <item name="android:minHeight">6.0dip</item>
        <item name="android:thumb">@drawable/thumb_bar_green</item>

    </style>

    <style name="PopupWindowAnimationStyle">
        <item name="android:windowEnterAnimation">@anim/tran_in</item>
        <item name="android:windowExitAnimation">@anim/tran_out</item>
    </style>

    <style name="PopupWindowAnimationStyle2">
        <item name="android:windowEnterAnimation">@anim/tran_in2</item>
        <item name="android:windowExitAnimation">@anim/tran_out2</item>
    </style>
</resources>
